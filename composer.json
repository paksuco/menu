{
    "name": "paksuco/menu",
    "description": "A menu implementation and rendering package",
    "keywords": [
        "menu",
        "laravel",
        "php",
        "oop"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Taha PAKSU",
            "email": "tpaksu@gmail.com",
            "homepage": "https://rettica.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/support": "^7.0",
        "illuminate/config": "^7.0",
        "illuminate/view": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Paksuco\\Menu\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Paksuco\\Menu\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Paksuco\\Menu\\ServiceProvider"
            ],
            "aliases": {
                "Package": "Paksuco\\Menu\\Facade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
